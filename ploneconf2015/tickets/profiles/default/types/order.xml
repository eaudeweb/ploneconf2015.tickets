<?xml version="1.0"?>
<object name="order" meta_type="Dexterity FTI" i18n:domain="ploneconf2015"
   xmlns:i18n="http://xml.zope.org/namespaces/i18n">
 <property name="title" i18n:translate="">Order</property>
 <property name="description"
    i18n:translate="">Plone Conference 2015 - Tickets Order</property>
 <property name="icon_expr">string:${portal_url}/document_icon.png</property>
 <property name="factory">ploneconf2015.tickets.content.order</property>
 <property name="add_view_expr">string:${folder_url}/++add++order</property>
 <property name="link_target"></property>
 <property name="immediate_view">order-view</property>
 <property name="global_allow">True</property>
 <property name="filter_content_types">True</property>
 <property name="allowed_content_types"/>
 <property name="allow_discussion">False</property>
 <property name="default_view">order-view</property>
 <property name="view_methods">
  <element value="order-view"/>
 </property>
 <property name="default_view_fallback">False</property>
 <property name="add_permission">cmf.AddPortalContent</property>
 <property name="klass">ploneconf2015.tickets.content.order.Order</property>
 <property name="behaviors">
  <element value="plone.app.content.interfaces.INameFromTitle"/>
  <element value="plone.app.dexterity.behaviors.metadata.IBasic"/>
  <element value="plone.app.dexterity.behaviors.exclfromnav.IExcludeFromNavigation" />
 </property>
 <property
    name="schema">ploneconf2015.tickets.content.interfaces.IOrder</property>
 <property name="model_source"></property>
 <property name="model_file"></property>
 <property name="schema_policy">dexterity</property>
 <alias from="(Default)" to="(dynamic view)"/>
 <alias from="edit" to="@@edit"/>
 <alias from="sharing" to="@@sharing"/>
 <alias from="view" to="(selected layout)"/>
 <action title="View" action_id="view" category="object" condition_expr=""
    description="" icon_expr="" link_target="" url_expr="string:${object_url}"
    visible="True">
  <permission value="View"/>
 </action>
 <action title="Edit" action_id="edit" category="object" condition_expr=""
    description="" icon_expr="" link_target=""
    url_expr="string:${object_url}/edit" visible="True">
  <permission value="Modify portal content"/>
 </action>
</object>
